<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="city" catalog="world" class="City">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="dbversion" catalog="world" class="DbVersion">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="userinfo" catalog="world" class="UserInfo">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="student" catalog="world" class="Student"></table>
	<table name="department" catalog="world" class="Department"></table>
</hibernate-reverse-engineering>